{
  "name": "vue-typescript-boilerplate",
  "version": "0.1.0",
  "private": false,
  "description": "A Vue boilerplate written in TypeScript.",
  "author": "Ustym Ukhman <ustym.ukhman@gmail.com>",
  "scripts": {
    "start": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production && node prerender.js",
    "lint": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "locale:extract": "node node_modules/vue-webpack-gettext/extract --output locale/template.pot --src src",
    "locale:compile": "node node_modules/vue-webpack-gettext/compile --output src/assets/data/locale --src locale/translated --multiple"
  },
  "dependencies": {
    "@hughsk/fulltilt": "^0.7.1",
    "@types/lodash": "^4.14.138",
    "@types/markdown-it": "^0.0.9",
    "animejs": "^3.1.0",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "breakpoint-sass": "^2.7.1",
    "console-polyfill": "^0.3.0",
    "core-js": "^3.6.4",
    "intersection-observer": "^0.7.0",
    "markdown-it-link-attributes": "^3.0.0",
    "mobile-detect": "^1.4.3",
    "node-reset-scss": "^1.0.1",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.2",
    "vue-axios": "^2.1.4",
    "vue-cookie": "^1.1.4",
    "vue-event-handler": "^1.1.8",
    "vue-gettext": "^2.1.5",
    "vue-meta": "^2.2.2",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@types/modernizr": "^3.5.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "minimist": "^1.2.5",
    "node-sass": "^4.9.0",
    "sass-loader": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-prerender-spa": "^1.1.6",
    "vue-template-compiler": "^2.6.10",
    "vue-webpack-gettext": "^0.3.1",
    "webpack-modernizr-loader": "^5.0.0"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "multilanguage": false,
  "domain": ""
}
